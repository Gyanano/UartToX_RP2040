; SPI PIO 程序 - 简化版
; 使用硬件 SPI 替代

.program spi_cpha0

.wrap_target
    nop
.wrap

% c-sdk {
static inline void pio_spi_init(PIO pio, uint sm, uint prog_offs,
                                 uint n_bits, uint freq,
                                 uint pin_sck, uint pin_mosi, uint pin_miso) {
    (void)pio; (void)sm; (void)prog_offs;
    (void)n_bits; (void)freq;
    (void)pin_sck; (void)pin_mosi; (void)pin_miso;
}
%}
